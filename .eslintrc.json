{
  "env": {
      "browser": true,
      "es2022": true
  },
  "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:@typescript-eslint/recommended",
      "prettier",
      "next"
  ],
  "overrides": [],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "plugins": [
      "react",
      "@typescript-eslint"
  ],
  "settings": {
  "react": {
    "version": "detect"
  }
},
  "ignorePatterns": ["src/**/*.test.{ts,tsx}", "node_modules"],
  "rules": {
      "react/react-in-jsx-scope": "off"
  }
}